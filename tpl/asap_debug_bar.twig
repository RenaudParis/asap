<style type="text/css">
	#asapDebugBar
	{
		position: fixed;
		bottom: 0;
		left: 50%;
		margin: 0;
		padding: 0;
		list-style: none;
		width: 1080px;
		margin-left: -540px;
		background: #401580; /* Old browsers */
		background: -moz-linear-gradient(top,  #401580 0%, #8c3fff 28%, #8c3fff 37%, #55338c 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#401580), color-stop(28%,#8c3fff), color-stop(37%,#8c3fff), color-stop(100%,#55338c)); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(top,  #401580 0%,#8c3fff 28%,#8c3fff 37%,#55338c 100%); /* Chrome10+,Safari5.1+ */
		background: -o-linear-gradient(top,  #401580 0%,#8c3fff 28%,#8c3fff 37%,#55338c 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(top,  #401580 0%,#8c3fff 28%,#8c3fff 37%,#55338c 100%); /* IE10+ */
		background: linear-gradient(top,  #401580 0%,#8c3fff 28%,#8c3fff 37%,#55338c 100%); /* W3C */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#401580', endColorstr='#55338c',GradientType=0 ); /* IE6-9 */
		color: white;
		text-shadow: 0 1px black;
		border-radius: 5px 5px 0 0;
		border: 2px solid #401580;
		border-bottom: 0;
		font-family: "Century Gothic", "Gill Sans MT", Arial, sans-serif;
		letter-spacing: 1px;
	}

	#asapDebugBar ul
	{
		margin: 0;
		padding: 0;
	}

	#asapDebugBar li
	{
		display: inline-block;
		height: 36px;
		line-height: 36px;
		border-left: 1px solid #9E5EFF;
		border-right: 1px solid #401580;
		vertical-align: middle;
		font-size: 12px;
		color: #FFFFFF;
		text-shadow: 0 1px #401580;
		padding: 0 10px;
		margin: 0;
	}

	#asapDebugBar li[category]:hover, #asapDebugBar li[category].active
	{
		cursor: pointer;
		background: #8c3fff;
	}

	#asapDebugBar li:first-child
	{
		border-left: none;
	}

	#asapDebugBar li:last-child
	{
		border-right: none;
	}

	#asapDebugBar li#asapVersion
	{
		font-size: 16px;
		color: #401580;
		text-shadow: 0 1px #9E5EFF;
	}

	#asapDebugBar .icon
	{
		display: inline-block;
		width: 30px;
		height: 30px;
		background-repeat: no-repeat;
		background-position: center center;
		border-radius: 15px;
		border: 1px outset gray;
		vertical-align: middle;
		margin-bottom: 3px;
		margin-right: 6px;
	}

	#asapDebugBar .iconImport
	{
		background-image: url(data:image/png;base64,);
	}

	#asapDebugBar .iconMemory
	{
		background-image: url(data:image/png;base64,);
	}

	#asapDebugBar .iconTime
	{
		background-image: url(data:image/png;base64,);
	}

	#asapDebugBar .iconDatabase
	{
		background-image: url(data:image/png;base64,);
	}

	#asapDebugBar .iconFlash
	{
		background-image: url(data:image/png;base64,);
	}

	#asapDebugBar .iconConfig
	{
		background-image: url(data:image/png;base64,);
	}

	#asapDebugBar .iconCache
	{
		background-image: url(data:image/png;base64,);
	}

	#asapDebugBar .asapDebugBarInfoCat
	{
		display: none;
	}

	#asapDebugBar #asapDebugBarInfo
	{
		display: none;
		position: absolute;
		bottom: 38px;
		left: 30px;
		width: 1020px;
		color: black;
		text-shadow: 0 1px #9E5EFF;
		padding-top: 26px;
	}

	#asapDebugBar #asapDebugBarInfo #asapDebugBarInfoContainer
	{
		background: #8c3fff;
		border-radius: 5px 0 0 0;
		border: 1px solid #401580;
		border-bottom: 0;
		overflow: auto;
		height: 280px;
		padding: 10px;
	}

	#asapDebugBar #asapDebugBarInfo #asapDebugBarInfoClose
	{
		position: absolute;
		top: 2px;
		right: 0;
		width: 30px;
		height: 24px;
		background: #8c3fff;
		border-radius: 5px 5px 0 0;
		border: 1px solid #401580;
		border-bottom: 0;
		z-index: 2;
		cursor: pointer;
		background-image: url(data:image/png;base64,);
		background-position: top center;
		background-repeat: no-repeat;
	}

	#asapDebugBar #asapDebugBarInfo #asapDebugBarInfoClose:hover
	{
		background-position: bottom center;
	}

	#asapDebugBar table
	{
		border-collapse: collapse;
	}

	#asapDebugBar table td, #asapDebugBar table th
	{
		border: 1px solid #9E5EFF;
		padding: 4px;
		font-size: 12px;
		color: black;
	}

	#asapDebugBar .showBacktrace
	{
		text-decoration: underline;
		cursor: pointer;
	}

	#asapDebugBar .showBacktrace:hover
	{
		text-decoration: none;
	}

	#asapDebugBar .backtrace
	{
		display: none;
	}

	#asapDebugBar .xdebug-var-dump
	{
		text-shadow: none;
		background: #CC99FF;
	}

</style>
<script type="text/javascript">
	if (window['$'] == undefined)
		alert('The ASAP DebugBar needs jQuery');
	else
	{
		var SAsapDebugBar =
		{
			debugBar: null,
			infoDiv: null,
			currentCat: '',


			onReady: function()
			{
				SAsapDebugBar.debugBar = $('#asapDebugBar');
				SAsapDebugBar.infoDiv = $('#asapDebugBarInfo', SAsapDebugBar.debugBar);
				$('#asapDebugBarInfoClose', SAsapDebugBar.infoDiv).click(SAsapDebugBar.hideInfo);
				$('.showBacktrace', SAsapDebugBar.debugBar).click(SAsapDebugBar.showBacktrace);
				$('li[category]', SAsapDebugBar.debugBar).click(SAsapDebugBar.onCatClick);
			},

			showBacktrace: function()
			{
				$('~ .backtrace', this).toggle();
			},

			onCatClick: function()
			{
				SAsapDebugBar.showCategory($(this).attr('category'));
			},

			showCategory: function(sCategory)
			{
				$('li[category]', SAsapDebugBar.debugBar).removeClass('active');
				if (SAsapDebugBar.currentCat == sCategory)
					return SAsapDebugBar.hideInfo();

				$('li[category=' + sCategory + ']', SAsapDebugBar.debugBar).addClass('active');
				$('.asapDebugBarInfoCat', SAsapDebugBar.debugBar).hide();
				$('#asapDebugBarInfoCat_' + sCategory, SAsapDebugBar.debugBar).show();
				SAsapDebugBar.infoDiv.slideDown();
				SAsapDebugBar.currentCat = sCategory;
			},

			hideInfo: function()
			{
				$('li[category]', SAsapDebugBar.debugBar).removeClass('active');
				SAsapDebugBar.infoDiv.slideUp();
				SAsapDebugBar.currentCat = '';
			}
		};

		$(document).ready(function()
		{
			SAsapDebugBar.onReady();
		});
	}
</script>
{% set allFlash = app.allFlash %}
{% spaceless %}
<div id="asapDebugBar">
	<ul>
		<li id="asapVersion">ASAP {{ constant('ASAP_VERSION') }}</li>
		<li category="time"><span class="icon iconTime"></span>Time {{ debugBar.nb('time') }}ms</li>
		<li category="memory"><span class="icon iconMemory"></span>Memory {{ debugBar.nb('memory') }}Mo</li>
		<li category="database"><span class="icon iconDatabase"></span>Queries {{ debugBar.nb('database') }}</li>
		<li category="imports"><span class="icon iconImport"></span>Imports {{ debugBar.nb('imports') }}</li>
		<li category="flash"><span class="icon iconFlash"></span>Flash {{ allFlash|length }}</li>
		<li category="cache"><span class="icon iconCache"></span>Cache {{ debugBar.nb('cache') }}</li>
		<li category="config"><span class="icon iconConfig"></span>Config</li>
	</ul>
	<div id="asapDebugBarInfo">
		<div id="asapDebugBarInfoClose" title="Close information panel"></div>
		<div id="asapDebugBarInfoContainer">
			<div class="asapDebugBarInfoCat" id="asapDebugBarInfoCat_time">
				Page execution time (PHP) : {{ debugBar.nb('time') }}ms
			</div>
			<div class="asapDebugBarInfoCat" id="asapDebugBarInfoCat_memory">
				{% set tmp = debugBar.info('memory') %}
				Current memory usage : {{ tmp.current }}Mo<br />
				Max memory usage : {{ tmp.max }}Mo
			</div>
			<div class="asapDebugBarInfoCat" id="asapDebugBarInfoCat_database">
				{% set tmp = debugBar.info('database') %}
				Amount of queries : {{ debugBar.nb('database') }}<br /><br />
				<table>
					<thead>
						<tr>
							<th>#</th>
							<th>Query</th>
							<th>Params</th>
							<th>Begin</th>
							<th>Duration</th>
							<th>Backtrace</th>
						</tr>
					</thead>
					<tbody>
					{% for idx, query in tmp %}
						<tr>
							<th>{{ idx + 1 }}</th>
							<td>{{ query.query }}</td>
							<td>{% for key, val in query.params %}{{ key }} = {{ val }}<br />{% endfor %}</td>
							<td>{{ query.time }}ms</td>
							<td>{{ query.duration }}ms</td>
							<td><span class="showBacktrace">Show backtrace</span><br /><span class="backtrace">{% debug query.backtrace %}</span></td>
						</tr>
					{% else %}
						<tr><td colspan="6">No queries</td></tr>
					{% endfor %}
					</tbody>
				</table>
			</div>
			<div class="asapDebugBarInfoCat" id="asapDebugBarInfoCat_imports">
				Amount of included/required files : {{ debugBar.nb('imports') }}<br /><br />
				{% for val in debugBar.info('imports') %}
					{{ val }}<br />
				{% endfor %}
			</div>
			<div class="asapDebugBarInfoCat" id="asapDebugBarInfoCat_flash">
				Amount of flash variables : {{ allFlash|length }}<br /><br />
				{% for key, val in allFlash %}
					{{ key }} = {{ val }}<br />
				{% endfor %}
			</div>
			<div class="asapDebugBarInfoCat" id="asapDebugBarInfoCat_config">
				env : <b>{{ asap.environment }}</b><br />
				debug : <b>{{ asap.debug ? 'active' : 'inactive' }}</b><br />
				<br />
				Config :
				{% debug asap.conf %}
			</div>
			<div class="asapDebugBarInfoCat" id="asapDebugBarInfoCat_cache">
				{% set tmp = debugBar.info('cache') %}
				Amount of keys : {{ debugBar.nb('cache') }}<br />
				{% set iTotal = 0 %}
				{% for type, typeCache in tmp %}
					{% for key, nb in typeCache %}
						{% set iTotal = iTotal + nb %}
					{% endfor %}
				{% endfor %}
				Total amount of access : {{ iTotal }}<br />
				<br />
				<table>
					<thead>
						<tr>
							<th>Type</th>
							<th>Key</th>
							<th>Access</th>
						</tr>
					</thead>
					<tbody>
					{% for type, typeCache in tmp %}
						{% for key, nb in typeCache %}
							<tr>
								<td>{{ type }}</td>
								<td>{{ key }}</td>
								<td>{{ nb }}</td>
							</tr>
						{% endfor %}
					{% else %}
						<tr><td colspan="6">No cache access</td></tr>
					{% endfor %}
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>
{% endspaceless %}
